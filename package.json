    "@types/glob": "^7.1.2",
    "@types/marked": "^1.1.0",
    "@types/prismjs": "^1.16.1",
    "chalk": "^4.1.0",
    "copy-text-to-clipboard": "^2.2.0",
    "cross-fetch": "^3.0.5",
    "dotenv": "^8.2.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "ionicons": "^5.1.2",
    "marked": "^1.1.0",
    "natives": "^1.1.6",
    "prismic-javascript": "^2.7.1",
    "prismjs": "^1.20.0",
    "remark": "^12.0.0",
    "rollup-plugin-dotenv": "^0.3.0",
